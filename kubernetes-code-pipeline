sudo docker rmi docker.io/nirbhaisingh/<name of image>
sudo ssh -o StrictHostKeyChecking=no -i passwd.pem root@<>  git clone https://github.com/singhnirbhai/<name of the repo>
sudo ssh -o StrictHostKeyChecking=no -i passwd.pem root@<>  kubectl create -f service.yaml  
sudo ssh -o StrictHostKeyChecking=no -i passwd.pem root@<>  kubectl create -f Deployment.yaml   
